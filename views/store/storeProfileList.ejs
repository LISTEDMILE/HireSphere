<%- include('../partials/head')%>
<link rel="stylesheet" href="/hostJobListStyle.css"/>
    <body>
      <%- include('../partials/navStore')%>
        <div id="body">
    <div id="div">
        <h1 id="top">Here are the added Profiles</h1>
        <div >
        <% details.forEach(function(detail) { %>
            <li>
              <div id="topIn">
                <h1 id="name">
                    <%= detail.profilePost %>
                    
                </h1>
                <div>

                <a href="/store/editProfile/<%= detail._id%>?editing=true">✏️</a>

                <form action="/store/deleteProfile/<%=detail._id%>" method='POST'>
                  <button type="submit">
                    ✘
                  </button>
              </form>
            </div>

              </div>
              <h2 id="Cname">
                <%=detail.profileName%>
              </h2>
              <h2 id="Salary">
                10th(%) : 
                <%=detail.profileTenth%>
              </h2>
              <h2 id="Location">
                12th(%) : 
                <%=detail.profileTwelth%>
              </h2>
              <label class="label1">
                Skills
              </label>
              <h2 id="Skills">
                <%=detail.profileSkills%>
              </h2>
                
                
                <a id="openDetails" href="/store/storeProfileDetails/<%= detail._id %>">More..</a>
                
                
            </li>
        <% });%>
    </div>
        <p> <b><u> test reached last </u></b></p>
    </div>
</div>
<%- include ('../partials/footer')%>
    </body>
</html>


